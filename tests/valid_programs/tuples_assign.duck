use go "fmt";

fn main() {
    let x: () = ();
    let y: (String,) = ("Tuple",);
    let z: (String, Int) = ("tuple 2", 10);

    go {
        fmt.Println(x)
        fmt.Println(y)
        fmt.Println(z)
    }

    z.0 = f"New string: {z.0}";

    go {
        fmt.Println(x)
        fmt.Println(y)
        fmt.Println(z)
    }
}
