use go "fmt";

fun abc(x: String) {
    go {
        fmt.Println(x)
    }
}

fun xyz(x: { x: String }) {
    let y: String = x.x;
    go {
        fmt.Println(y);
    }
}

fun main() {
    let x: struct { y: String } = .{ y: "Hallo" };
    let y: { x: { x: String } } = { x: { x: "Hallo" } };
    abc(y.x.x);
    xyz(y.x);
}
