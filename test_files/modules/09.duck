use go "fmt";

module X {
    use Y::{c};

    fun a() -> XDuck {
        ::abc();
        {
            x: 10,
        }
    }
    module Y {
        fun b() {
            ::X::a();
        }

        fun c() {
            go {
                fmt.Println("Hallo von c")
            }
        }

        fun testi() {
            go {
                fmt.Println("testi")
            }
        }

        module ZZ {
            fun zz() {
                go {
                    fmt.Println("ZZ")
                }
            }
        }

    }

    fun call_c() {
        c();
        Y::testi();
        Y::ZZ::zz();
    }

    type XDuck = {
        x: Int,
    };

}

use X::Y::ZZ::{zz};
use X::Y::{ZZ};

fun main() {
    X::a();
    X::Y::b();
    X::call_c();
    X::Y::ZZ::zz();
    zz();
    go {
        fmt.Println(1)
    }
    ZZ::zz();
}

fun abc() {
    go {
        fmt.Println("hell from abc")
    }
}
